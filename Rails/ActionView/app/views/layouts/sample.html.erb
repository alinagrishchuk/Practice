<!DOCTYPE html>
<html>
<head>
  <title>ActionView</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="row">
    <div class="col-md-8">
      <div class="alert alert-success" role="alert">
        <%= yield %>
      </div>
    </div>
  </div>
</body>
</html>